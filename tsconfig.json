{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "lib": ["es5", "es6", "dom"], 
    "jsx": "react",
    "sourceMap": false,
    "outDir": "./built",
    "strictPropertyInitialization": false,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true
  }
}
